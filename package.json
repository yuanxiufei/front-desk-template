{
  "name": "front-desk-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,vue}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,vue}\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/core": "^8.1.2",
    "ali-oss": "^6.17.0",
    "axios": "^1.8.4",
    "cropperjs": "^1.5.12",
    "dayjs": "^1.11.0",
    "driver.js": "^0.9.8",
    "file-saver": "^2.0.5",
    "gsap": "^3.9.1",
    "md5": "^2.3.0",
    "vee-validate": "^4.5.10",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.3.0",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.24.0",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "vue-eslint-parser": "^9.3.2",
    "typescript": "^5.0.2",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.8",
    "prettier": "^3.0.3",
    "sass": "^1.45.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.0.23",
    "vite": "^6.2.3",
    "vite-plugin-svg-icons": "^0.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "front-desk-template": "0.0.0"
  }
}
